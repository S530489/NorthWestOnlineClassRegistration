<% include adminHeader %>

    <ul>
        <li><a href="adminHome">Profile</a></li>
        <li><a href="adminSearch">Search</a></li>
        <li><a href="addCourse"  class="active">Add Course</a></li>
        <li><a href="updateDeleteCourse">Update/Delete</a></li>
        <li style="float:right"><a href="/">Logout</a></li>
    </ul>

    <br>
    <br>

    <form method="post" action="/addCourse">
        <div class="row">
            <div class="col-md-4"></div>
            <div class="col-md-4">
                <div class="form-group">
                    <label>Course Name:</label>
                    <input type="text" class="form-control" name="coursename">
                </div>
                <div class="form-group">
                    <label>Course Id:</label>
                    <input type="text" class="form-control" name="courseid">
                </div>
                <div class="form-group">
                    <label>Location</label>
                    <input type="text" class="form-control"  name="location">
                </div>
                <div class="form-group">
                    <label>Start Date:</label>
                    <input type="text" class="form-control"  name="startdate">
                </div>
                <div class="form-group">
                    <label>End Date</label>
                    <input type="text" class="form-control"  name="enddate">
                </div>
                <div class="form-group">
                        <label>Timings</label>
                        <input type="text" class="form-control"  name="time">
                    </div>
                <div class="form-group">
                    <label>No, of Seats:</label>
                    <input type="text" class="form-control"  name="seats">
                </div>
    

    
            </div>
        </div>
        <br>
        <div class="row">
            <div class="col-md-1"></div>
            &emsp;&emsp;&emsp;&emsp;
            <div class="col-md-4"></div>
            <button type="submit" class="btn btn-primary">Submit</button>
    
        </div>
    </form>

</body>

</html>